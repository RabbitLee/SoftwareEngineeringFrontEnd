</div>

<div class = "focus-bar" id="#head-bar">
    <div class="title"><i class="fa fa-user-circle"></i> 个人中心</div>
</div>
<div class="tab-container">
    <div class="tabs-vertical">

        <ul id="tab-nav">
            <li>
                <a class="tab-active nav-button" data-index="0" href="#">我的路线</a>
            </li>
        </ul>

        <div class="tabs-content-placeholder">

            <section class="tab-content-active" id="getRoute">
                <h1>我接包的路线</h1>
                <div class="content" >
                    <div class="container">
                        <div class="square-container">
                            <div id="list"><table id="data_table" class="table">
                                    <thead>
                                    <tr>
                                        <th>创建者</th>
                                        <th>开始日期</th>
                                        <th>结束日期</th>
                                        <th>目的城市</th>
                                        <th>开始景点</th>
                                        <th>结束景点</th>
                                        <th>&nbsp;</th>
                                    </tr>
                                    </thead>
                                </table>
                            </div>


                        </div>
                        <div class="page-container">
                            <div id="page"></div>
                        </div>

                    </div>
                </div>

                <div id="RouteContent">
                    <div class="routeWrapper">
                        <p class="plan-title"><b></b><span>详细路线信息 <i class="fa fa-calendar-check-o"></i></span><b></b></p>
                        <div id="dateLabel"></div>
                        <div id="cityLabel"></div><br>
                        <div id="creatorLabel"></div>
                        <div id="othersLabel"></div>
                        <div id="routeLabel"></div>
                        <div id="RouteContainer" style="position: static; width:90%; height:600px" ></div>
                        <div id="vote" class="vote-container">
                            已竞价旅行社：<br><br>
                            <div id="agencyLabel"></div>
                        </div>
                        <div class="bid-container">
                            <label>您的出价:  </label>
                            <input id="money" name="money" required="required" type="text" placeholder="输入金额">

                            </input><i class="fa fa-jpy"></i>
                        </div>
                        <button id="bid" onclick="bid()" type="button">修改出价</button>
                        <button id="backAll" onclick="backAll()" type="button">返回</button>
                    </div>

                </div>
            </section>
            <button id="exitAgency" onclick="exit()" type="button">退出账号</button>
        </div>

    </div>
</div>

<!--headroom-->
<script src="js/index.js"></script>
