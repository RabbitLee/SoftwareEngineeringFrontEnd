    <ul class = "navbar">
        <li><a href='<%= whetherLoginHref %>' class="signIn item-anchor"><i class="fa fa-user-circle user-icon"></i><%= whetherLogin %></a></li>
        <!--<li><a href="#" class="discover item-anchor">发现</a></li>-->
        <li><a href="/square" class="team item-anchor">广场</a></li>
        <li><a href="/route" class="start item-anchor">开始规划</a></li>
        <li><a href="/index" class="index item-anchor">首页</a></li>
    </ul>
</div>

<div class = "head-bar" id="head-bar">
    <div><i class="fa fa-heartbeat"></i> 发现 , 心动的旅程</div>
</div>

<div class="content" >
    <div class="container">
        <div class="square-container">

            <p class="plan-title"><b></b><span>所有路线列表</span><b></b></p>
            <div id="list"><table id="data_table" class="table">
                    <thead>
                    <tr>
                        <th>创建者</th>
                        <th>开始日期</th>
                        <th>结束日期</th>
                        <th>目的城市</th>
                        <th>开始景点</th>
                        <th>结束景点</th>
                        <th>&nbsp;</th>
                        </tr>
                    </thead>
                </table>
            </div>
            <div class="page-container">
                <div id="page"></div>
            </div>

        </div>

    </div>
</div>

<div id="RouteContent">
    <div class="routeWrapper">
        <p class="plan-title"><b></b><span>详细路线信息 <i class="fa fa-calendar-check-o"></i></span><b></b></p>
        <div id="dateLabel"></div>
        <div id="cityLabel"></div><br>
        <div id="creatorLabel"></div>
        <div id="othersLabel"></div>
        <div id="routeLabel"></div>
        <div id="RouteContainer" style="position: static; width:90%; height:600px" ></div>
        <div id="vote" class="vote-container">
            候选旅行社：<br><br>
            <div id="agencyLabel"></div>
        </div>
        <button id="joinRoute" onclick="joinRoute()" type="button">加入</button>
        <button id="voteFor" onclick="voteFor()" type="button">投票</button>
        <button id="backAll" onclick="backAll()" type="button">返回</button>
        <div id="voteinfo"></div>
    </div>

</div>




<script src="js/square.js"></script>
<script src="js/index.js"></script>
<script type="text/javascript" src="http://webapi.amap.com/demos/js/liteToolbar.js"></script>

